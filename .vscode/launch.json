	"version": "0.2.0",
	"configurations": [
		{
			"args": [
				"check-status"
			],
			"name": "Gulp check-status",
			"program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**",
				"${workspaceFolder}/**/node_modules/**",
				"${workspaceFolder}/node_modules/**",
				"${workspaceFolder}\\**\\node_modules\\**\\*.js",
				"${workspaceFolder}\\node_modules\\**\\*.js",
			],
			"type": "pwa-node",
			"outputCapture": "std",
		}

	]
}